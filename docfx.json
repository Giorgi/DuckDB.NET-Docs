{
  "metadata": [
    {
      "src": [
        {
          "src": "../DuckDB.NET",
          "files": [
            "**/Bindings.csproj",
            "**/Data.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "my-template"
    ],
    "globalMetadata": {
      "_appName": "DuckDB.NET",
      "_appTitle": "DuckDB.NET",
      "_enableSearch": true,
      "pdf": false
    }
  }
}